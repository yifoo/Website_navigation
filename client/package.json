{
  "private": true,
  "scripts": {
    "dev": "UMI_ENV=dev umi dev",
    "pre": "UMI_ENV=pre umi dev",
    "analyze": "ANALYZE=1 umi dev",
    "ab": "ANALYZE=1 UMI_ENV=pro umi build",
    "build": "UMI_ENV=pro umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/charts": "^1.3.1",
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-form": "^1.49.3",
    "@ant-design/pro-layout": "^6.5.0",
    "@ant-design/pro-table": "^2.58.1",
    "@umijs/plugin-esbuild": "^1.3.1",
    "crypto-js": "^4.1.1",
    "jsonp": "^0.2.1",
    "react": "17.x",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "17.x",
    "react-lazyload": "^3.2.0",
    "umi": "^3.5.20"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.17",
    "compression-webpack-plugin": "^9.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}
