{
  "name": "mynav",
  "version": "4.0.0",
  "private": true,
  "description": "简约的个人导航",
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "react"
      ]
    }
  },
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && umi build",
    "deploy": "npm run build && npm run gh-pages",
    "dev": "export NODE_OPTIONS=--openssl-legacy-provider && npm run start:dev",
    "pre": "export NODE_OPTIONS=--openssl-legacy-provider && npm run start:pre",
    "gh-pages": "gh-pages -d dist",
    "i18n-remove": "pro i18n-remove --locale=zh-CN --write",
    "postinstall": "umi g tmp",
    "lint:prettier": "prettier -c --write \"src/**/*\" --end-of-line auto",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "playwright": "playwright install && playwright test",
    "prepare": "husky install",
    "prettier": "prettier -c --write \"src/**/*\"",
    "serve": "umi-serve",
    "start": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:dev": "cross-env MOCK=none REACT_APP_ENV=dev UMI_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none UMI_ENV=dev umi dev",
    "start:no-ui": "cross-env UMI_UI=none UMI_ENV=dev umi dev",
    "start:pre": "cross-env REACT_APP_ENV=pre UMI_ENV=dev umi dev",
    "start:test": "cross-env REACT_APP_ENV=test MOCK=none UMI_ENV=dev umi dev",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "test:e2e": "node ./tests/run-tests.js",
    "tsc": "tsc --noEmit",
    "commit:comment": "引导设置规范化的提交信息",
    "commit": "git-cz"
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-card": "^1.20.7",
    "@ant-design/pro-components": "^1.1.3",
    "@ant-design/pro-descriptions": "^1.11.3",
    "@ant-design/pro-form": "^1.69.1",
    "@ant-design/pro-layout": "^6.38.6",
    "@ant-design/pro-table": "^2.76.1",
    "@umijs/route-utils": "^2.1.1",
    "ahooks": "^3.4.0",
    "antd": "^4.24.12",
    "classnames": "^2.3.1",
    "crypto-js": "^4.1.1",
    "hex-alpha": "^1.3.0",
    "jsonp": "^0.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "omit.js": "^2.0.2",
    "qs": "^6.10.3",
    "rc-menu": "^9.6.0",
    "rc-overflow": "^1.3.0",
    "rc-util": "^5.33.0",
    "react": "^17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-colorful": "^5.5.1",
    "react-dev-inspector": "^1.8.0",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^1.3.0",
    "react-lazyload": "^3.2.0",
    "umi": "^3.5.24"
  },
  "devDependencies": {
    "@ant-design/pro-cli": "^2.1.5",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@playwright/test": "^1.22.2",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.11",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.182",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "@types/react-helmet": "^6.1.5",
    "@umijs/fabric": "^2.10.0",
    "@umijs/openapi": "^1.5.1",
    "@umijs/plugin-blocks": "^2.2.2",
    "@umijs/plugin-esbuild": "^1.4.1",
    "@umijs/preset-ant-design-pro": "^1.3.3",
    "@umijs/preset-react": "^2.1.3",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "compression-webpack-plugin": "^10.0.0",
    "cross-env": "^7.0.3",
    "cross-port-killer": "^1.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "detect-installer": "^1.0.2",
    "eslint": "^7.32.0",
    "gh-pages": "^3.2.3",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^10.5.4",
    "mockjs": "^1.1.0",
    "prettier": "^2.6.2",
    "typescript": "^4.7.2",
    "umi-serve": "^1.9.11"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
